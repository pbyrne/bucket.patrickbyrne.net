{
  "name": "bucket.patrickbyrne.net",
  "description": "Source code powering my image bucket",
  "homepage": "https://bucket.patrickbyrne.net",
  "license": "See LICENSE file",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbyrne/bucket.patrickbyrne.net.git"
  },
  "engines": {
    "node": "~18.12.1",
    "npm": "~8.19.2"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-fetch": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.14.54",
    "liquidjs": "^10.6.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:eleventy": "npx @11ty/eleventy",
    "build:esbuild": "npx esbuild --bundle --outdir=_site/javascripts/ source/javascripts/*",
    "clean": "rm -rf _site",
    "serve": "npm-run-all clean --parallel serve:*",
    "serve:eleventy": "npx @11ty/eleventy --serve",
    "serve:esbuild": "npx esbuild --outdir=_site/javascripts/ --watch --sourcemap source/javascripts/*"
  }
}
